generator client {
    provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") 
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Event{
  Id String @id @default(cuid())
  EventName String
  EventDate String
  EventTime String
  Venue String
  TicketLinks String
  SocialLinks String
  Description String
  InquiryNumber String
  FlyerImagePath String
  IsEventWeekly Boolean
  DayofWeek String 
}

model AdminUser{
  id String @id @default(cuid())
  username String @unique
  passwords String
}